begin

end;

procedure TSong.setAlbum;
begin

end;

procedure TSong.setArtist;
begin

end;

procedure TSong.setDuration;
begin

end;

procedure TSong.setSong;
begin

end;

procedure TSong.setTrackNumber;
begin

end;

function TSong.toString: String;
begin

end;

end.
